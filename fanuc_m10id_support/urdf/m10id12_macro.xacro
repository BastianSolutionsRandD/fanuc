<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:include filename="$(find fanuc_resources)/urdf/common_materials.xacro"/>

  <xacro:macro name="fanuc_m10id12" params="prefix">
    <!-- links: main serial chain -->
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/base_link.stl"/>
        </geometry>
        <xacro:material_fanuc_gray40 />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/base_link.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_1.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_1.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_2.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_2.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_3.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_3.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_4.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_4.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_5.stl"/>
        </geometry>
        <xacro:material_fanuc_yellow />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_5.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/link_6.stl"/>
        </geometry>
        <xacro:material_fanuc_black />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/link_6.stl"/>
        </geometry>
      </collision>
    </link>

<!-- DCS ZONE LINKS: 5 zone links starting at link_2 -->
    <link name="dcs_z1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/dcs_z1.stl"/>
        </geometry>
        <!-- <xacro:material_fanuc_black /> -->
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/dcs_z1.stl"/>
        </geometry>
      </collision>
    </link>

    <link name="dcs_z2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/dcs_z2.stl"/>
        </geometry>
        <!-- <xacro:material_fanuc_black /> -->
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/dcs_z2.stl"/>
        </geometry>
      </collision>
    </link>

    <link name="dcs_z3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/dcs_z3.stl"/>
        </geometry>
        <!-- <xacro:material_fanuc_black /> -->
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/dcs_z3.stl"/>
        </geometry>
      </collision>
    </link>

    <link name="dcs_z4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/dcs_z4.stl"/>
        </geometry>
        <!-- <xacro:material_fanuc_black /> -->
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/dcs_z4.stl"/>
        </geometry>
      </collision>
    </link>

    <link name="dcs_z5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/visual/dcs_z5.stl"/>
        </geometry>
        <!-- <xacro:material_fanuc_black /> -->
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://fanuc_m10id_support/meshes/m10id12/collision/dcs_z5.stl"/>
        </geometry>
      </collision>
    </link>

<!--//TODO: Verify ranges are correct-->
    <!-- joints: main serial chain -->
    <joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.3035" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 1"/>
      <limit lower="-2.967" upper="2.967" effort="0" velocity="4.53"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
      <origin xyz="0.075 0 0.1465" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 1 0"/>
      <limit lower="-1.57" upper="2.53" effort="0" velocity="4.18"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
      <origin xyz="0 0 0.640" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 -1 0"/>
      <limit lower="-3.14" upper="4.66" effort="0" velocity="4.53"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
      <origin xyz="0.2555 0 0.195" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="-1 0 0"/>
      <limit lower="-3.31" upper="3.31" effort="0" velocity="7.50"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <origin xyz="0.4445 -0.0572 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 -1 0"/>
      <limit lower="-2.443" upper="2.443" effort="0" velocity="7.85"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
      <origin xyz="0.075 0.0572 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="-1 0 0"/>
      <limit lower="-3.22886" upper="3.22886" effort="0" velocity="12.56"/>
    </joint>

    <!-- ROS-Industrial 'base' frame: base_link to Fanuc World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.450" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange" />
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}link_6" />
      <child link="${prefix}flange" />
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}link_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${pi} ${-pi/2.0} 0" />
      <parent link="${prefix}flange" />
      <child link="${prefix}tool0" />
    </joint>


<!-- DCS JOINTS -->
    <joint name="dcs_z1-joint" type="fixed">
      <origin xyz="-0.175 0 0.127" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="dcs_z1"/>
    </joint>

    <joint name="dcs_z2-joint" type="fixed">
      <origin xyz="-0.035 -0.04 0.02" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="dcs_z2"/>
    </joint>

    <joint name="dcs_z3-joint" type="fixed">
      <origin xyz="0.085 0.01 -0.14" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="dcs_z3"/>
    </joint>

    <joint name="dcs_z4-joint" type="fixed">
      <origin xyz="0.041 0.07 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="dcs_z4"/>
    </joint>

    <joint name="dcs_z5-joint" type="fixed">
      <origin xyz="0.035 0.057 -0.065" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="dcs_z5"/>
    </joint>


  </xacro:macro>
</robot>
